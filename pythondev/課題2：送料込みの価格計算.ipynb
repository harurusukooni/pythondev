    "exclude=int(input(\"税抜価格を入力してください：\"))\n",
    "print(\"税抜価格を入力してください：\" ,exclude, )\n",
    "include=int(round(exclude*1.1,1))\n",
    "\n",
    "if include >=  2000:\n",
    "    print(\"送料は無料です\")\n",
    "\n",
    "else :\n",
    "    print(\"送料として350円かかります\")\n",
    "    include=include+350\n",
    "\n",
    "print(\"送料込みの価格は\" ,include, \"円です。\")"
   ]
